<div class="container index-container">
    <h1>ZoekBee</h1>
</div>

<div class="container">
<h1>All posts</h1>
  <div class="row">
    <% Post.all.each do |post| %>
        <div class="col-sm">
        <%= render partial: 'partials/post', locals: {post: post} %>
        </div>
    <% end %>
  </div>
</div>






    
